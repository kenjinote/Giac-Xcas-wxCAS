    f2 -= df2*dt;
    res1:=res1,((j+1)*dt+i*f1);
    res2:=res2,(-(j+1)*dt+i*f2);
  }
  res:=op(revlist(res2)),res1;
  print(res);
  return open_polygon(res);
}:;
