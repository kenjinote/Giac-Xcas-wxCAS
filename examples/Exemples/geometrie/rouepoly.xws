tracer(n):={
  local A,B,j,Ls;
  Ls:=evalf(Lsarc(n));
  A:=[arc((Ls)[0],(Ls)[1],-2*pi/n)];
  for (j:=1;j<(n-1);j++){
    A:=concat(A,arc(Ls[j],Ls[j+1],evalf(-2*pi/n)));
  }
  B:=translation(n,A);
  return concat(A,B);
}:;,
