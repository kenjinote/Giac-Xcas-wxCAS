longtrajet(n):={
  local dist,k,C,R;
  dist:=0;
  for (k:=0;k<(n-1);k++){
    arc((Lsarc(n))[k],(Lsarc(n))[k+1],-pi/3,C,R);
    dist:=dist+evalf(R*pi/3);
  };
  return normal(dist);
}:;,
